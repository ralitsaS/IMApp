<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.katakiari.contest2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <service
            android:name=".CTService"
            android:exported="false" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LOGGEDIN"
            android:label="@string/app_name" />
        <activity
            android:name=".REGIN"
            android:label="@string/title_activity_regin" >
        </activity>
        <activity
            android:name=".PrivateChat"
            android:label="@string/title_activity_private_chat" >
        </activity>
        <activity
            android:name=".TabbedActivity"
            android:label="@string/title_activity_tabbed" >
        </activity>
        <activity
            android:name=".AccountActivity"
            android:label="@string/title_activity_account" >
        </activity>
        <activity
            android:name=".PublicChat"
            android:label="@string/title_activity_public_chat" >
        </activity>
        <activity
            android:name=".GroupChat"
            android:label="@string/title_activity_group_chat" >
        </activity>
    </application>

</manifest>
